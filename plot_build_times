set output 'images/build.png'
set term png enhanced font "Vera,11" 
set log x
set xtics rotate by 90 
set xlabel "Input Size in MBs" 
set ylabel "Time in seconds"
set xtics (800, 1600, 3200, 6400, 12800, 25600, 51200, 102400, 204800, 409600, 819200, 1638400)
plot 'data/build_time' using 1:3 with linespoint title "Our implementation", 'data/build_time' using 1:4 with linespoint title "B+ Tree", 'data/build_time' using 1:5 with linespoint title "MySQL MyISAM";
